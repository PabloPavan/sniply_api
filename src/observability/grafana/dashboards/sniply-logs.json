{
  "__inputs": [
    {
      "name": "DS_LOKI",
      "label": "Loki",
      "description": "",
      "type": "datasource",
      "pluginId": "loki",
      "pluginName": "Loki"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "10.0.0"
    },
    {
      "type": "datasource",
      "id": "loki",
      "name": "Loki",
      "version": "1.0.0"
    },
    {
      "type": "panel",
      "id": "logs",
      "name": "Logs",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "timeseries",
      "title": "Logs per second (by service)",
      "id": 1,
      "gridPos": {
        "h": 7,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "loki",
        "uid": "${DS_LOKI}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (compose_service) (rate({compose_project=~\"$project\", compose_service=~\"$service\"}[1m]))"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      }
    },
    {
      "type": "logs",
      "title": "Logs (filtered)",
      "id": 2,
      "gridPos": {
        "h": 13,
        "w": 24,
        "x": 0,
        "y": 7
      },
      "datasource": {
        "type": "loki",
        "uid": "${DS_LOKI}"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{compose_project=~\"$project\", compose_service=~\"$service\"}"
        }
      ],
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": true,
        "showLabels": true,
        "sortOrder": "Descending"
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "sniply",
    "logs",
    "loki"
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_LOKI",
        "label": "Loki",
        "query": "loki",
        "current": {
          "text": "Loki",
          "value": "Loki"
        }
      },
      {
        "type": "query",
        "name": "project",
        "label": "Project",
        "datasource": "${DS_LOKI}",
        "query": "label_values(compose_project)",
        "includeAll": true,
        "allValue": ".+",
        "multi": true,
        "refresh": 2
      },
      {
        "type": "query",
        "name": "service",
        "label": "Service",
        "datasource": "${DS_LOKI}",
        "query": "label_values(compose_service)",
        "includeAll": true,
        "allValue": ".+",
        "multi": true,
        "refresh": 2
      }
    ]
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m"
    ],
    "time_options": [
      "5m",
      "15m",
      "1h",
      "6h",
      "12h",
      "24h"
    ]
  },
  "timezone": "",
  "title": "sniply_api Logs (Loki)",
  "uid": "sniply-logs",
  "version": 1
}
